      {
        "value": "io.github.lucaargolo.accuratemaps.client.AccurateMapsClient",
        "adapter": "kotlin"
      }
    ],
    "main": [
      {
        "value": "io.github.lucaargolo.accuratemaps.AccurateMaps",
        "adapter": "kotlin"
      }
